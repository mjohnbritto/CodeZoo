<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
    version="6.0">
  <runtime>java21</runtime> <!-- or another supported version -->
  <servlet>
    <servlet-name>FlowController</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
  </servlet>
  <!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.pages.algorithm.FibonacciModal_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.algorithm.FibonacciModal_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.algorithm.TriangularModal_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.algorithm.TriangularModal_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.algorithm.SortModal_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.algorithm.SortModal_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.Contact_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.Contact_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.Dashboard_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.Dashboard_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.algorithm.SwapModal_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.algorithm.SwapModal_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.About_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.About_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.PerformanceAnalyzer_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.PerformanceAnalyzer_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.HomePage_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.HomePage_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.algorithm.SearchModal_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.algorithm.SearchModal_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.resetPassword_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.resetPassword_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.persist.CourseConceptsDetails_jsp</servlet-name>
        <servlet-class>org.apache.jsp.persist.CourseConceptsDetails_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.pages.Forum_jsp</servlet-name>
        <servlet-class>org.apache.jsp.pages.Forum_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.persist.QuizDetails_jsp</servlet-name>
        <servlet-class>org.apache.jsp.persist.QuizDetails_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.algorithm.FibonacciModal_jsp</servlet-name>
        <url-pattern>/pages/algorithm/FibonacciModal.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.algorithm.TriangularModal_jsp</servlet-name>
        <url-pattern>/pages/algorithm/TriangularModal.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.algorithm.SortModal_jsp</servlet-name>
        <url-pattern>/pages/algorithm/SortModal.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.Contact_jsp</servlet-name>
        <url-pattern>/pages/Contact.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.Dashboard_jsp</servlet-name>
        <url-pattern>/pages/Dashboard.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.algorithm.SwapModal_jsp</servlet-name>
        <url-pattern>/pages/algorithm/SwapModal.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.About_jsp</servlet-name>
        <url-pattern>/pages/About.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.PerformanceAnalyzer_jsp</servlet-name>
        <url-pattern>/pages/PerformanceAnalyzer.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.HomePage_jsp</servlet-name>
        <url-pattern>/pages/HomePage.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.algorithm.SearchModal_jsp</servlet-name>
        <url-pattern>/pages/algorithm/SearchModal.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.resetPassword_jsp</servlet-name>
        <url-pattern>/pages/resetPassword.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.persist.CourseConceptsDetails_jsp</servlet-name>
        <url-pattern>/persist/CourseConceptsDetails.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.pages.Forum_jsp</servlet-name>
        <url-pattern>/pages/Forum.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.persist.QuizDetails_jsp</servlet-name>
        <url-pattern>/persist/QuizDetails.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<servlet-mapping>
    <servlet-name>FlowController</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <app-engine-apis>true</app-engine-apis>
</web-app>